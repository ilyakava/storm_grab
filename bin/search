#!/usr/bin/env ruby

require 'csv'
require_relative '../search_architizer'

file = ARGV[0]

queries_array = []

puts 'reading input file'

CSV.foreach(file) do |row|
  queries_array << row.join(' ')
end

puts "if percentage count stalls, press 'Control+c' and check internet availibility"

SearchArchitizer.new(queries_array, csv = true)
